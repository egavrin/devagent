{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://devagent.ai/schemas/react_observation.json",
  "title": "ReActObservation",
  "type": "object",
  "required": ["success"],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "outcome": {
      "type": "string",
      "default": ""
    },
    "metrics": {
      "type": "object",
      "additionalProperties": true,
      "default": {}
    },
    "artifacts": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "default": []
    },
    "tool": {
      "type": ["string", "null"],
      "default": null
    },
    "raw_output": {
      "type": ["string", "null"],
      "default": null
    },
    "error": {
      "type": ["string", "null"],
      "default": null
    }
  },
  "additionalProperties": true
}
