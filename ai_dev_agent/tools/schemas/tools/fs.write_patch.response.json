{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "fs.write_patch response",
  "type": "object",
  "required": ["applied", "diff_stats"],
  "properties": {
    "applied": { "type": "boolean" },
    "rejected_hunks": {
      "type": "array",
      "items": { "type": "string" }
    },
    "new_files": {
      "type": "array",
      "items": { "type": "string" }
    },
    "changed_files": {
      "type": "array",
      "items": { "type": "string" }
    },
    "diff_stats": {
      "type": "object",
      "required": ["lines", "files"],
      "properties": {
        "lines": { "type": "integer", "minimum": 0 },
        "files": { "type": "integer", "minimum": 0 }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
