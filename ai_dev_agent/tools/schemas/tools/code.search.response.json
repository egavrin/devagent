{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "code.search response",
  "type": "object",
  "required": ["matches"],
  "properties": {
    "matches": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["path", "line", "col", "preview"],
        "properties": {
          "path": { "type": "string" },
          "line": { "type": "integer", "minimum": 1 },
          "col": { "type": "integer", "minimum": 1 },
          "preview": { "type": "string" },
          "structure": {
            "type": "object",
            "properties": {
              "kind": { "type": "string" },
              "symbol": { "type": "string" },
              "depth": { "type": "integer", "minimum": 0 },
              "line": { "type": "integer", "minimum": 1 },
              "path": {
                "type": "array",
                "items": { "type": "string" }
              },
              "ancestors": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "kind": { "type": "string" },
                    "name": { "type": "string" },
                    "line": { "type": "integer", "minimum": 1 }
                  },
                  "additionalProperties": false
                }
              },
              "summary": { "type": "string" }
            },
            "additionalProperties": true
          },
          "structure_summary": { "type": "string" },
          "import_context": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "additionalProperties": false
      }
    }
  },
  "additionalProperties": false
}
